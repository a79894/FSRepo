 var mylibrary = function(){
 			
 			CheckForPhone = function() {
 			if (index(3) === "-" && index(7) === "-") {
                return true;
            } else {
                return false;
            }
 	}
 	
 	
 
 	
 	return {
 	"CheckForPhone" : CheckForPhone
 	}
 
 }
 
 var newLib = new myLibrary
 
 console.log ("Is this a Phone number?" + newLib.CheckForPhone("405-476-8216"));